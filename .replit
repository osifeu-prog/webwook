run = "python main.py"
language = "python3"
entrypoint = "main.py"

[packager]
language = "python3"

[packager.features]
packageSearch = true
guessImports = true

[env]
BOT_TOKEN = "YOUR_BOT_TOKEN_HERE"
WEBHOOK_URL = "https://your-repl-name.your-username.repl.co"
GIT_REPO_URL = "https://github.com/yourusername/your-repo.git"
